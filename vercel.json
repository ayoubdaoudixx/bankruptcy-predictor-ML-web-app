{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "static/",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "build": {
    "env": {
      "PIP_CACHE_DIR": "/tmp/.pip-cache",
      "PIP_NO_CACHE_DIR": "1",
      "PIP_IGNORE_INSTALLED": "1"
    }
  }
}